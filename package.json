		"jest": "29.7.0",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"react": "18.3.1",
		"standard-version": "8.0.0",
		"ts-jest": "29.2.5"
	},
	"dependencies": {
		"@babel/core": "7.25.8",
		"@babel/plugin-syntax-class-properties": "7.12.13",
