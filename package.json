	"license": "MIT",
	"keywords": [
		"proptypes",
		"typescript",
		"react"
	],
	"scripts": {
		"test": "jest test/index.test.ts",
		"test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
